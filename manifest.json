{
	
  "manifest_version": 2,
  "name": "Action filter",
  "description": "DÃ©place les messages en fonction de filtres sur action",
  "version": "1.0",
  "default_locale": "fr",
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "78",
	  "id": "florent.mouneau@filter.com"
    }
  },

	"author": "Florent Mouneau",
	
  "background": {
    "scripts": ["background.js"]
  },
  
  "permissions": [
    "menus",
    "messagesRead",
    "messagesMove",
    "activeTab",
    "tabs",
    "tabHide",
    "accountsFolders",
    "accountsRead",
    "downloads",
    "activeTab",
	"storage"
  ],
  
  
  "options_ui": {
  "page": "option.html",
  "browser_style": true
},
   "browser_action": {
    "default_icon": {
      "16": "icons/page-16.png",
      "32": "icons/page-32.png"
    },
	"default_title" :"filtre option"
},

	"message_display_action": {
		"default_icon": {
		  "16": "icons/page-16.png",
		  "32": "icons/page-32.png"
		},
		"default_title" :"action"
	},

  "icons": {
    "16": "icons/page-16.png",
    "32": "icons/page-32.png",
    "48": "icons/page-48.png"
  },
  
	"commands": {
		"range": {
			"suggested_key": {
				"default": "Ctrl+Shift+X"
			},
			"description": "Range Maile"
		}
	}

}
